/* pages/login.wxss */
.imgcontainer{
  width: 100vw;
  height: 340rpx;
  background-image: url('data:image/png;base64,');
  background-position: bottom center;
  background-size: contain;
  background-repeat: no-repeat;
}

.form {
  margin-top: 86rpx;
  width: 100vw;
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 32rpx;
}
.form-item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 110rpx;
  border: 1rpx solid #E1E1E1;
  border-radius: 8rpx;
  margin-bottom: 30rpx;
  padding: 0 40rpx;
}
input {
  flex: 1;
  color: #1E2C32;
  font-size: 32rpx;
  border: none;
}
.phcolor {
  color: #A0A0A0;
  font-size: 32rpx;
}

.code{
  width: 180rpx;
  display: flex;
  color: #F8875F;
  justify-content: center;
}

.submit {
  background: linear-gradient(to right, #FFB154, #FF6850);
  color: #fff;
  height: 110rpx;
  text-align: center;
  line-height: 110rpx;
  border-radius: 8rpx;
  margin-top: 100rpx;
}
.submitDisable {
  background: #D0D0D0;
}
